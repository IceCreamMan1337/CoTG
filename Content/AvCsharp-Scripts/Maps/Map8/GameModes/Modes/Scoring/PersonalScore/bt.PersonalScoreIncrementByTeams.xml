<?xml version="1.0" encoding="utf-8"?>
<BehaviorTrees>
  <BehaviorTree Name="Modes.Dominion.Scoring.PersonalScore.PersonalScoreIncrementByTeams">
    <Parameters>
      <Parameter Name="TeamToAwardPoints" VariableType="Reference, Value" ParameterType="TeamEnum" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="ReferencePosition" VariableType="Reference" ParameterType="Vector" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="ScoreValue" VariableType="Reference, Value" ParameterType="Float" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="ScoreCategory" VariableType="Reference, Value" ParameterType="ScoreCategory" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="ScoreEvent" VariableType="Reference, Value" ParameterType="ScoreEvent" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="Radius" VariableType="Reference, Value" ParameterType="Float" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="IncStat" VariableType="Reference, Value" ParameterType="Bool" Default="" Optional="false">
        <Description>When set to true, we'll inc the statEvent, otherwise we won't</Description>
      </Parameter>
      <Parameter Name="StatEvent" VariableType="Reference, Value" ParameterType="StatEvent" Default="" Optional="true">
        <Description></Description>
      </Parameter>
      <Parameter Name="CheckBuff" VariableType="Reference, Value" ParameterType="Bool" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="ScoreWithBuff" VariableType="Reference, Value" ParameterType="Float" Default="" Optional="true">
        <Description></Description>
      </Parameter>
      <Parameter Name="BuffToCheck" VariableType="Reference, Value" ParameterType="String" Default="" Optional="true">
        <Description></Description>
      </Parameter>
      <Parameter Name="IncStatWithBuff" VariableType="Reference, Value" ParameterType="Bool" Default="" Optional="false">
        <Description>use the inc stat with buff</Description>
      </Parameter>
      <Parameter Name="StatWithBuff" VariableType="Reference, Value" ParameterType="StatEvent" Default="" Optional="true">
        <Description></Description>
      </Parameter>
    </Parameters>
    <Node Type="Sequence" Name="Sequence">
      <Parameters />
      <Children>
        <Node Type="Selector" Name="Selector">
          <Parameters />
          <Children>
            <Node Type="Sequence" Name="TeamIsOrder">
              <Parameters />
              <Children>
                <Node Type="EqualUnitTeam" Name="TeamToAwardPoints==Order">
                  <Parameters>
                    <Parameter Name="LeftHandSide" Value="TeamToAwardPoints" VariableType="Reference" ReferenceType="TeamEnum" />
                    <Parameter Name="RightHandSide" Value="TEAM_ORDER" VariableType="Value" />
                  </Parameters>
                </Node>
                <Node Type="GetTurret" Name="ReferenceUnit">
                  <Parameters>
                    <Parameter Name="Team" Value="TEAM_ORDER" VariableType="Value" />
                    <Parameter Name="Lane" Value="0" VariableType="Value" />
                    <Parameter Name="Position" Value="1" VariableType="Value" />
                    <OutParameter Name="Turret" Value="ReferenceUnit" ReferenceType="AttackableUnit" />
                  </Parameters>
                </Node>
              </Children>
            </Node>
            <Node Type="Sequence" Name="TeamIsChaos">
              <Parameters />
              <Children>
                <Node Type="EqualUnitTeam" Name="TeamToAwardPoints==Chaos">
                  <Parameters>
                    <Parameter Name="LeftHandSide" Value="TeamToAwardPoints" VariableType="Reference" ReferenceType="TeamEnum" />
                    <Parameter Name="RightHandSide" Value="TEAM_CHAOS" VariableType="Value" />
                  </Parameters>
                </Node>
                <Node Type="GetTurret" Name="ReferenceUnit">
                  <Parameters>
                    <Parameter Name="Team" Value="TEAM_CHAOS" VariableType="Value" />
                    <Parameter Name="Lane" Value="0" VariableType="Value" />
                    <Parameter Name="Position" Value="1" VariableType="Value" />
                    <OutParameter Name="Turret" Value="ReferenceUnit" ReferenceType="AttackableUnit" />
                  </Parameters>
                </Node>
              </Children>
            </Node>
          </Children>
        </Node>
        <Node Type="GetUnitsInTargetArea" Name="Unit=ToGivePoints">
          <Parameters>
            <Parameter Name="Unit" Value="ReferenceUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
            <Parameter Name="TargetLocation" Value="ReferencePosition" VariableType="Reference" ReferenceType="Vector" />
            <Parameter Name="Radius" Value="Radius" VariableType="Reference" ReferenceType="Float" />
            <Parameter Name="SpellFlags" Value="AffectEnemies,AffectFriends,AffectHeroes,AffectNeutral" VariableType="Value" />
            <OutParameter Name="Output" Value="ToGivePoints" ReferenceType="AttackableUnitCollection" />
          </Parameters>
        </Node>
        <Node Type="IterateOverAllDecorator" Name="IterateOverAllDecorator">
          <Parameters>
            <Parameter Name="Collection" Value="ToGivePoints" VariableType="Reference" ReferenceType="AttackableUnitCollection" />
            <OutParameter Name="Output" Value="IndividualPointUnit" ReferenceType="AttackableUnit" />
          </Parameters>
          <Children>
            <Node Type="Sequence" Name="Sequence">
              <Parameters />
              <Children>
                <Node Type="GetUnitTeam" Name="UnitTeam">
                  <Parameters>
                    <Parameter Name="Unit" Value="IndividualPointUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
                    <OutParameter Name="Output" Value="UnitTeam" ReferenceType="TeamEnum" />
                  </Parameters>
                </Node>
                <Node Type="EqualUnitTeam" Name="UnitTeam==TeamToAwardPoints">
                  <Parameters>
                    <Parameter Name="LeftHandSide" Value="UnitTeam" VariableType="Reference" ReferenceType="TeamEnum" />
                    <Parameter Name="RightHandSide" Value="TeamToAwardPoints" VariableType="Reference" ReferenceType="TeamEnum" />
                  </Parameters>
                </Node>
                <Node Type="Selector" Name="Selector">
                  <Parameters />
                  <Children>
                    <Node Type="Sequence" Name="TestBuff">
                      <Parameters />
                      <Children>
                        <Node Type="EqualBool" Name="CheckBuff==True">
                          <Parameters>
                            <Parameter Name="LeftHandSide" Value="CheckBuff" VariableType="Reference" ReferenceType="Bool" />
                            <Parameter Name="RightHandSide" Value="True" VariableType="Value" />
                          </Parameters>
                        </Node>
                        <Node Type="TestUnitHasBuff" Name="HasBuff">
                          <Parameters>
                            <Parameter Name="TargetUnit" Value="IndividualPointUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
                            <Parameter Name="CasterUnit" Value="" VariableType="Reference" ReferenceType="AttackableUnit" />
                            <Parameter Name="BuffName" Value="BuffToCheck" VariableType="Reference" ReferenceType="String" />
                            <Parameter Name="ReturnSuccessIf" Value="True" VariableType="Value" />
                          </Parameters>
                        </Node>
                        <Node Type="IncrementPlayerScore" Name="IncrementPlayerScore">
                          <Parameters>
                            <Parameter Name="Player" Value="IndividualPointUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
                            <Parameter Name="ScoreCategory" Value="ScoreCategory" VariableType="Reference" ReferenceType="ScoreCategory" />
                            <Parameter Name="ScoreEvent" Value="ScoreEvent" VariableType="Reference" ReferenceType="ScoreEvent" />
                            <Parameter Name="Value" Value="ScoreWithBuff" VariableType="Reference" ReferenceType="Float" />
                            <Parameter Name="Show" Value="" VariableType="Reference" ReferenceType="Bool" />
                          </Parameters>
                        </Node>
                        <Node Type="MaskFailure" Name="IncStat">
                          <Parameters />
                          <Children>
                            <Node Type="Sequence" Name="Sequence">
                              <Parameters />
                              <Children>
                                <Node Type="EqualBool" Name="IncStatWithBuff==True">
                                  <Parameters>
                                    <Parameter Name="LeftHandSide" Value="IncStatWithBuff" VariableType="Reference" ReferenceType="Bool" />
                                    <Parameter Name="RightHandSide" Value="True" VariableType="Value" />
                                  </Parameters>
                                </Node>
                                <Node Type="IncrementPlayerStat" Name="IncrementPlayerStat">
                                  <Parameters>
                                    <Parameter Name="Player" Value="IndividualPointUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
                                    <Parameter Name="StatEvent" Value="StatWithBuff" VariableType="Reference" ReferenceType="StatEvent" />
                                  </Parameters>
                                </Node>
                              </Children>
                            </Node>
                          </Children>
                        </Node>
                      </Children>
                    </Node>
                    <Node Type="Sequence" Name="Sequence">
                      <Parameters />
                      <Children>
                        <Node Type="GreaterFloat" Name="ScoreValue&gt;0">
                          <Parameters>
                            <Parameter Name="LeftHandSide" Value="ScoreValue" VariableType="Reference" ReferenceType="Float" />
                            <Parameter Name="RightHandSide" Value="0" VariableType="Value" />
                          </Parameters>
                        </Node>
                        <Node Type="IncrementPlayerScore" Name="IncrementPlayerScore">
                          <Parameters>
                            <Parameter Name="Player" Value="IndividualPointUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
                            <Parameter Name="ScoreCategory" Value="ScoreCategory" VariableType="Reference" ReferenceType="ScoreCategory" />
                            <Parameter Name="ScoreEvent" Value="ScoreEvent" VariableType="Reference" ReferenceType="ScoreEvent" />
                            <Parameter Name="Value" Value="ScoreValue" VariableType="Reference" ReferenceType="Float" />
                            <Parameter Name="Show" Value="" VariableType="Reference" ReferenceType="Bool" />
                          </Parameters>
                        </Node>
                        <Node Type="MaskFailure" Name="IncStat">
                          <Parameters />
                          <Children>
                            <Node Type="Sequence" Name="Sequence">
                              <Parameters />
                              <Children>
                                <Node Type="EqualBool" Name="IncStat==True">
                                  <Parameters>
                                    <Parameter Name="LeftHandSide" Value="IncStat" VariableType="Reference" ReferenceType="Bool" />
                                    <Parameter Name="RightHandSide" Value="True" VariableType="Value" />
                                  </Parameters>
                                </Node>
                                <Node Type="IncrementPlayerStat" Name="IncrementPlayerStat">
                                  <Parameters>
                                    <Parameter Name="Player" Value="IndividualPointUnit" VariableType="Reference" ReferenceType="AttackableUnit" />
                                    <Parameter Name="StatEvent" Value="StatEvent" VariableType="Reference" ReferenceType="StatEvent" />
                                  </Parameters>
                                </Node>
                              </Children>
                            </Node>
                          </Children>
                        </Node>
                      </Children>
                    </Node>
                  </Children>
                </Node>
              </Children>
            </Node>
          </Children>
        </Node>
      </Children>
    </Node>
  </BehaviorTree>
</BehaviorTrees>