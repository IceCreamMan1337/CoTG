<?xml version="1.0" encoding="utf-8"?>
<BehaviorTrees>
  <BehaviorTree Name="Modes.Dominion.Sigil.RelicHelper">
    <Parameters>
      <Parameter Name="RelicSquadID" VariableType="Reference" ParameterType="Int" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="RelicSpawnTime" VariableType="Reference" ParameterType="Float" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="RelicCampID" VariableType="Reference, Value" ParameterType="Int" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="RelicEncounterShield" VariableType="Reference" ParameterType="Int" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="RelicPosition" VariableType="Reference" ParameterType="Vector" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <OutParameter Name="RelicSpawnTime" ParameterType="Float">
        <Description></Description>
      </OutParameter>
      <OutParameter Name="RelicSquadID" ParameterType="Int">
        <Description></Description>
      </OutParameter>
    </Parameters>
    <Node Type="MaskFailure" Name="MaskFailure">
      <Parameters />
      <Children>
        <Node Type="Sequence" Name="Sequence">
          <Parameters />
          <Children>
            <Node Type="GetGameTime" Name="CurrentTime">
              <Parameters>
                <OutParameter Name="Output" Value="CurrentTime" ReferenceType="Float" />
              </Parameters>
            </Node>
            <Node Type="Selector" Name="Spawn_Or_CheckSpawn">
              <Parameters />
              <Children>
                <Node Type="Sequence" Name="Spawn">
                  <Parameters />
                  <Children>
                    <Node Type="LessEqualFloat" Name="RelicSpawnTime&lt;=CurrentTime">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="RelicSpawnTime" VariableType="Reference" ReferenceType="Float" />
                        <Parameter Name="RightHandSide" Value="CurrentTime" VariableType="Reference" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                    <Node Type="AddFloat" Name="RelicSpawnTime=CurrentTime+50000">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="CurrentTime" VariableType="Reference" ReferenceType="Float" />
                        <Parameter Name="RightHandSide" Value="50000" VariableType="Value" />
                        <OutParameter Name="Output" Value="RelicSpawnTime" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                    <Node Type="SpawnNeutralCampFromEncounter" Name="RelicSquadID">
                      <Parameters>
                        <Parameter Name="EncounterId" Value="RelicEncounterShield" VariableType="Reference" ReferenceType="Int" />
                        <Parameter Name="Position" Value="RelicPosition" VariableType="Reference" ReferenceType="Vector" />
                        <Parameter Name="Icon" Value="HealthPack" VariableType="Value" />
                        <Parameter Name="CampId" Value="RelicCampID" VariableType="Reference" ReferenceType="Int" />
                        <Parameter Name="ExistingSquadId" Value="" VariableType="Reference" ReferenceType="Int" />
                        <Parameter Name="SquadName" Value="" VariableType="Reference" ReferenceType="String" />
                        <OutParameter Name="SquadId" Value="RelicSquadID" ReferenceType="Int" />
                      </Parameters>
                    </Node>
                  </Children>
                </Node>
                <Node Type="Sequence" Name="CheckSpawn">
                  <Parameters />
                  <Children>
                    <Node Type="AddFloat" Name="UpperBound">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="CurrentTime" VariableType="Reference" ReferenceType="Float" />
                        <Parameter Name="RightHandSide" Value="25000" VariableType="Value" />
                        <OutParameter Name="Output" Value="UpperBound" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                    <Node Type="GreaterFloat" Name="RelicSpawnTime&gt;UpperBound">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="RelicSpawnTime" VariableType="Reference" ReferenceType="Float" />
                        <Parameter Name="RightHandSide" Value="UpperBound" VariableType="Reference" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                    <Node Type="GreaterEqualInt" Name="RelicSquadID&gt;=0">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="RelicSquadID" VariableType="Reference" ReferenceType="Int" />
                        <Parameter Name="RightHandSide" Value="0" VariableType="Value" />
                      </Parameters>
                    </Node>
                    <Node Type="TestSquadIsAlive" Name="TestNoRelic">
                      <Parameters>
                        <Parameter Name="SquadId" Value="RelicSquadID" VariableType="Reference" ReferenceType="Int" />
                        <Parameter Name="ReturnSuccessIf" Value="False" VariableType="Value" />
                      </Parameters>
                    </Node>
                    <Node Type="GenerateRandomFloat" Name="NewRelicSpawnTime">
                      <Parameters>
                        <Parameter Name="MinValue" Value="30" VariableType="Value" />
                        <Parameter Name="MaxValue" Value="30" VariableType="Value" />
                        <OutParameter Name="Output" Value="NewRelicSpawnTime" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                    <Node Type="AddFloat" Name="RelicSpawnTime=CurrentTime+NewRelicSpawnTime">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="NewRelicSpawnTime" VariableType="Reference" ReferenceType="Float" />
                        <Parameter Name="RightHandSide" Value="CurrentTime" VariableType="Reference" ReferenceType="Float" />
                        <OutParameter Name="Output" Value="RelicSpawnTime" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                  </Children>
                </Node>
              </Children>
            </Node>
          </Children>
        </Node>
      </Children>
    </Node>
  </BehaviorTree>
</BehaviorTrees>