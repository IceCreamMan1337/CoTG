<?xml version="1.0" encoding="utf-8"?>
<BehaviorTrees>
  <BehaviorTree Name="Modes.Dominion.GuardianCaptureMechanic.ObeliskCapturePointGoldRewards">
    <Parameters>
      <Parameter Name="TeamToGiveGold" VariableType="Reference, Value" ParameterType="TeamEnum" Default="" Optional="false">
        <Description></Description>
      </Parameter>
      <Parameter Name="ReferencePosition" VariableType="Reference" ParameterType="Vector" Default="" Optional="false">
        <Description></Description>
      </Parameter>
    </Parameters>
    <Node Type="MaskFailure" Name="MaskFailure">
      <Parameters />
      <Children>
        <Node Type="Sequence" Name="GiveGoldRewards">
          <Parameters />
          <Children>
            <Node Type="Selector" Name="Order_Chaos_Turret">
              <Parameters />
              <Children>
                <Node Type="Sequence" Name="Order_Turret">
                  <Parameters />
                  <Children>
                    <Node Type="EqualUnitTeam" Name="TeamToGiveGold==ORDER">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="TeamToGiveGold" VariableType="Reference" ReferenceType="TeamEnum" />
                        <Parameter Name="RightHandSide" Value="TEAM_ORDER" VariableType="Value" />
                      </Parameters>
                    </Node>
                    <Node Type="GetTurret" Name="ComparisonTurret">
                      <Parameters>
                        <Parameter Name="Team" Value="TEAM_ORDER" VariableType="Value" />
                        <Parameter Name="Lane" Value="0" VariableType="Value" />
                        <Parameter Name="Position" Value="1" VariableType="Value" />
                        <OutParameter Name="Turret" Value="ShrineTurret" ReferenceType="AttackableUnit" />
                      </Parameters>
                    </Node>
                  </Children>
                </Node>
                <Node Type="GetTurret" Name="ComparisonTurret">
                  <Parameters>
                    <Parameter Name="Team" Value="TEAM_CHAOS" VariableType="Value" />
                    <Parameter Name="Lane" Value="0" VariableType="Value" />
                    <Parameter Name="Position" Value="1" VariableType="Value" />
                    <OutParameter Name="Turret" Value="ShrineTurret" ReferenceType="AttackableUnit" />
                  </Parameters>
                </Node>
              </Children>
            </Node>
            <Node Type="GetUnitsInTargetArea" Name="GetUnitsInTargetArea">
              <Parameters>
                <Parameter Name="Unit" Value="ShrineTurret" VariableType="Reference" ReferenceType="AttackableUnit" />
                <Parameter Name="TargetLocation" Value="ReferencePosition" VariableType="Reference" ReferenceType="Vector" />
                <Parameter Name="Radius" Value="1200" VariableType="Value" />
                <Parameter Name="SpellFlags" Value="AffectFriends,AffectHeroes" VariableType="Value" />
                <OutParameter Name="Output" Value="LocalChampionCollection" ReferenceType="AttackableUnitCollection" />
              </Parameters>
            </Node>
            <Node Type="GetCollectionCount" Name="GetCollectionCount">
              <Parameters>
                <Parameter Name="Collection" Value="LocalChampionCollection" VariableType="Reference" ReferenceType="AttackableUnitCollection" />
                <OutParameter Name="Output" Value="LocalChampCount" ReferenceType="Int" />
              </Parameters>
            </Node>
            <Node Type="GreaterInt" Name="TestForChamps">
              <Parameters>
                <Parameter Name="LeftHandSide" Value="LocalChampCount" VariableType="Reference" ReferenceType="Int" />
                <Parameter Name="RightHandSide" Value="0" VariableType="Value" />
              </Parameters>
            </Node>
            <Node Type="SubtractInt" Name="ExtraChampCount">
              <Parameters>
                <Parameter Name="LeftHandSide" Value="LocalChampCount" VariableType="Reference" ReferenceType="Int" />
                <Parameter Name="RightHandSide" Value="1" VariableType="Value" />
                <OutParameter Name="Output" Value="ExtraChampCount" ReferenceType="Int" />
              </Parameters>
            </Node>
            <Node Type="MultiplyInt" Name="MultiplyInt">
              <Parameters>
                <Parameter Name="LeftHandSide" Value="ExtraChampCount" VariableType="Reference" ReferenceType="Int" />
                <Parameter Name="RightHandSide" Value="50" VariableType="Value" />
                <OutParameter Name="Output" Value="ExtraGold" ReferenceType="Int" />
              </Parameters>
            </Node>
            <Node Type="AddInt" Name="TotalGold">
              <Parameters>
                <Parameter Name="LeftHandSide" Value="100" VariableType="Value" />
                <Parameter Name="RightHandSide" Value="ExtraGold" VariableType="Reference" ReferenceType="Int" />
                <OutParameter Name="Output" Value="TotalGold" ReferenceType="Int" />
              </Parameters>
            </Node>
            <Node Type="DivideFloat" Name="CalcIndividualGold">
              <Parameters>
                <Parameter Name="LeftHandSide" Value="TotalGold" VariableType="Reference" ReferenceType="Int" />
                <Parameter Name="RightHandSide" Value="LocalChampCount" VariableType="Reference" ReferenceType="Int" />
                <OutParameter Name="Output" Value="IndividualGold" ReferenceType="Float" />
              </Parameters>
            </Node>
            <Node Type="IterateOverAllDecorator" Name="IterateOverAllDecorator">
              <Parameters>
                <Parameter Name="Collection" Value="LocalChampionCollection" VariableType="Reference" ReferenceType="AttackableUnitCollection" />
                <OutParameter Name="Output" Value="LocalChamp" ReferenceType="AttackableUnit" />
              </Parameters>
              <Children>
                <Node Type="Sequence" Name="Sequence">
                  <Parameters />
                  <Children>
                    <Node Type="GetUnitTeam" Name="UnitTeam">
                      <Parameters>
                        <Parameter Name="Unit" Value="LocalChamp" VariableType="Reference" ReferenceType="AttackableUnit" />
                        <OutParameter Name="Output" Value="UnitTeam" ReferenceType="TeamEnum" />
                      </Parameters>
                    </Node>
                    <Node Type="EqualUnitTeam" Name="UnitTeam==TeamToGiveGold">
                      <Parameters>
                        <Parameter Name="LeftHandSide" Value="UnitTeam" VariableType="Reference" ReferenceType="TeamEnum" />
                        <Parameter Name="RightHandSide" Value="TeamToGiveGold" VariableType="Reference" ReferenceType="TeamEnum" />
                      </Parameters>
                    </Node>
                    <Node Type="GiveChampionGold" Name="GiveChampionGold">
                      <Parameters>
                        <Parameter Name="Unit" Value="LocalChamp" VariableType="Reference" ReferenceType="AttackableUnit" />
                        <Parameter Name="GoldAmount" Value="IndividualGold" VariableType="Reference" ReferenceType="Float" />
                      </Parameters>
                    </Node>
                  </Children>
                </Node>
              </Children>
            </Node>
          </Children>
        </Node>
      </Children>
    </Node>
  </BehaviorTree>
</BehaviorTrees>